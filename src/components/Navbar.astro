    <!-- Header Wrap End  -->
  </div>
</div>
<!-- Header End -->

<!-- Offcanvas Start-->
<div class="offcanvas offcanvas-start" id="offcanvasExample">
  <div class="offcanvas-header">
    <!-- Offcanvas Logo Start -->
    <div class="offcanvas-logo">
      <a href="/"><img src="/logo-dd-w.png" alt=""></a>
    </div>
    <!-- Offcanvas Logo End -->
    <button type="button" class="close-btn" data-bs-dismiss="offcanvas"><i class="flaticon-close"></i></button>
  </div>

  <!-- Offcanvas Body Start -->
  <div class="offcanvas-body">
    <div class="offcanvas-menu">
      <ul class="main-menu">
        <li class="active-menu">
          <a href="/">Home</a>
          <ul class="sub-menu">
            <li class="active"><a href="/">Home Main</a></li>
            <li><a href="/index-2">AI Solutions</a></li>
            <li><a href="/index-3">Cyber Security</a></li>
            <li><a href="/index-4">IT Solutions</a></li>
            <li><a href="/index-5">Software Company</a></li>
            <li><a href="/index-6">IT Agency</a></li>
          </ul>
        </li>
        <li>
          <a href="/about">About Us</a>
        </li>
        <li><a href="#">Services</a>
          <ul class="sub-menu">
            <li><a href="/data-engineering">Data Engineering and AI Strategy</a></li>
            <li><a href="/service">Mobile application Development</a></li>
            <li><a href="/service">Web Development</a></li>
            <li><a href="/service">Cloud and DevOps</a></li>
          </ul>
        </li>
        <li><a href="#">Blog</a>
          <ul class="sub-menu">
            <li><a href="/blog">Blog Grid</a></li>
            <li><a href="/blog-standard">Blog List</a></li>
            <li><a href="/blog-details">Blog Single</a></li>
          </ul>
        </li>
        <li><a href="/contact">Contact</a></li>
      </ul>
    </div>
  </div>
  <!-- Offcanvas Body End -->
</div>
<!-- Offcanvas End -->

<script is:inline>
  // Header Sticky functionality
  window.addEventListener('scroll', function() {
    const header = document.getElementById('header');
    if (header) {
      if (window.pageYOffset > 50 || window.scrollY > 50) {
        header.classList.add('sticky');
      } else {
        header.classList.remove('sticky');
      }
    }
  });
</script>
